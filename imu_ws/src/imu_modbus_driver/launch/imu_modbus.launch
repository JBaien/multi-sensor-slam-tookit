<?xml version="1.0"?>
<!--
    IMU Modbus Driver - 基础启动文件
    功能: 仅启动IMU Modbus节点
-->
<launch>
    <node name="imu_modbus_node" pkg="imu_modbus_driver" type="imu_modbus_node"
          output="screen" respawn="true">
        <param name="ip_address" value="192.168.188.105" />
        <param name="port" value="502" />
    </node>
</launch>
